datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

model User {
  id  String @id @default(auto()) @map("_id") @db.ObjectId
  fid Int    @unique
}

model RequestLogs {
  id          String      @id @default(auto()) @map("_id") @db.ObjectId
  fid         Int
  url         String
  messageHash String
  timestamp   Int
  network     Int
  buttonIndex Int
  castId      CastId
  trustedData TrustedData
}

type CastId {
  fid  Int
  hash String
}

type TrustedData {
  messageBytes String
}
